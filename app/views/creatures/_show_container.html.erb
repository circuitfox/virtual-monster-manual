<div class="collapse-wrapper">
  <input id="<%= model %><%= id || "" %>-collapse" class="toggle" type="checkbox" checked>
  <label for="<%= model %><%= id || ""%>-collapse" class="toggle-label container collapse-header" tabindex=0>
    <h3><%= model.humanize %></h3>
    <span class="collapse-note push-right"><%# filled by css %></span>
  </label>
  <div class="collapse-content">
    <div class="collapse-inner">
      <%= yield %>
    </div>
  </div>
</div>
